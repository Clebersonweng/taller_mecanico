<h1>Listing invoice_details</h1>

<table>
  <thead>
    <tr>
      <th>Type of work</th>
      <th>Invoice master</th>
      <th>Order master</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoice_details.each do |invoice_detail| %>
      <tr>
        <td><%= invoice_detail.type_of_work_id %></td>
        <td><%= invoice_detail.invoice_master_id %></td>
        <td><%= invoice_detail.order_master_id %></td>
        <td><%= link_to 'Show', invoice_detail %></td>
        <td><%= link_to 'Edit', edit_invoice_detail_path(invoice_detail) %></td>
        <td><%= link_to 'Destroy', invoice_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Invoice detail', new_invoice_detail_path %>
